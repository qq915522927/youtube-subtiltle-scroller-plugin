{
    "manifest_version": 2,
    "name": "Subtitle navigation",
    "version": "0.1.4",
    "description": "TBD",
    "permissions": [
      "https://*.youtube.com/*",
      "webRequest",
      "webRequestBlocking",
      "tabs"
    ],
    "background": {
        "scripts": ["./dist/background.js"],
        "persistent": true
      },
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*"],
        "js": ["./dist/foreground.js"]
      }
    ]
}
